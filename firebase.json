{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    // Dynamic Links will start with https://echophotos.io/invite
    "appAssociation": "AUTO",
    "rewrites": [
      {
        "source": "/invite/**",
        "dynamicLinks": true
      }
    ]
  }
}
